{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.79",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.79",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.79/bin/apache-tomcat-9.0.79-windows-x64.zip",
            "hash": "sha512:7f72af407ff341e7329ecdaf61492f3405237b80da9516b4e95de502dd6ffad16c9d6bc4b867847415b4c3c2d8100bfe6efbfa211588fb1bfbc0cb3c0981edc0"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.79/bin/apache-tomcat-9.0.79-windows-x86.zip",
            "hash": "sha512:829cf38123149245eccdeb62f39ead758262ac4b28094ae8becbe6f67138a758743230049d49512d1c3de2a2c7ed7bb27f4abb8b8a6fb471a8c8df83cd7ce13f"
        }
    }
}
