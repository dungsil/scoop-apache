{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.82",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.82",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.82/bin/apache-tomcat-9.0.82-windows-x64.zip",
            "hash": "sha512:183b8ee94e47789f3ba3b1b4bd775ce4d17b7e53b9303ad432a5d9e8d9e5a4e6f18e8979b7ff3ccb6efc9e12d6293936835038646b21b5cc9dfdfb1f12b13dca"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.82/bin/apache-tomcat-9.0.82-windows-x86.zip",
            "hash": "sha512:bb623a62997920df7dcdc8948ea3a562731948feaaee56c121bdc71f2bd12ca58260b855952902c87aadf1aa97e1a2690670dc3ad6efe99d27995b2e0b687e3a"
        }
    }
}
