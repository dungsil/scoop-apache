{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.80",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.80",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.80/bin/apache-tomcat-9.0.80-windows-x64.zip",
            "hash": "sha512:a726e9982e74a33736b8e7e948c3dc5be13c4738320bcf1b326e726c061e3a921e72fc64091c0bdd7d80da03f9fa5cd17f6f5ace790dce53c069c73a0bb20c2a"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.80/bin/apache-tomcat-9.0.80-windows-x86.zip",
            "hash": "sha512:a62391a0621ca0ab5996cf3344c770cfb989aa4d1d3ad440590aec312fbc968f961fc4124c774945ffd6b53cc98130d5b722eefe180a39525eba99637f30ef8f"
        }
    }
}
