{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.55",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.55",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.55/bin/apache-tomcat-9.0.55-windows-x64.zip",
            "hash": "sha512:a66d5c9cfa845664a2421d3f5d0a26fce113ede25c7558ab63640cede9eee770843d67408f043fa6778af376c839efe790180238d3a30f874f6d0a2a9d5751f4"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.55/bin/apache-tomcat-9.0.55-windows-x86.zip",
            "hash": "sha512:773ecea30328c5a4ac1567718109e30aa1a94711fc36f4591c0ba99db755ca572162749c32261f425341a71bf04acb01b15b368f53d1a73040ed24404732fe1d"
        }
    }
}
