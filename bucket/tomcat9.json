{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.91",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.91",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.91/bin/apache-tomcat-9.0.91-windows-x64.zip",
            "hash": "sha512:51478f9d0b5f1659dc454a52a8e832fa971c9ca9f37278ddc4bd853a24fcc58ffbaa0b0c7ce878acab47e0dd9ae329a8e6d382cf7e746c5f6900a2b488bfb9c2"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.91/bin/apache-tomcat-9.0.91-windows-x86.zip",
            "hash": "sha512:f8a0ceb1076785f8e9a4c537e25604937ad39cdd588e171316529b5167b16b0193706510e7db9a5543c43aebfa92d66095a39d8a2b26d9ac88746e6bc94af11e"
        }
    }
}
