{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.85",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.85",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.85/bin/apache-tomcat-9.0.85-windows-x64.zip",
            "hash": "sha512:486f01ed68bed47328eafa21c32be5233b5a6809cd1c0d98a9abc7ba9b4f671a95663d00214d7140d21b13504e2edd78ab691c4c2176381eff452a246fc3d5b9"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.85/bin/apache-tomcat-9.0.85-windows-x86.zip",
            "hash": "sha512:ca11bd3d396cc36bffdc6a7276544f9adc260b69da511bb919de2361338cf3b0223d50f2d2ca8fa7332cdcbc6333bbbb4bbd8157625a48fbb92f70ea8fe17c80"
        }
    }
}
