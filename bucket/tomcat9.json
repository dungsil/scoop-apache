{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.84",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.84",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.84/bin/apache-tomcat-9.0.84-windows-x64.zip",
            "hash": "sha512:2f08b544a9223154a292f006bb8ee1fd840c514fd6b3830c7e78eb3833f840bf56e7c655d07c53c968478b33818ac61ddde981382cfde30c917f66df4a03e83c"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.84/bin/apache-tomcat-9.0.84-windows-x86.zip",
            "hash": "sha512:8ec8cd6a592fdd82c465897d166ac9e09ef480c668e65b2bc41a69c2910c9d69f43b06929f6fab5a19b1c99b8546ed8ad4d8481d200baa6fbd0558152142b4eb"
        }
    }
}
