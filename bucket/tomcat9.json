{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.76",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.76",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.76/bin/apache-tomcat-9.0.76-windows-x64.zip",
            "hash": "sha512:a96582ce2903cef810b9c9564661846ebdb9ecbd9d2ab9d286e646e1e7a8b9dac093ecb319de4a345b3a0ea00a05bebd566c81310c1f9a79360ff82d5cd3f33f"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.76/bin/apache-tomcat-9.0.76-windows-x86.zip",
            "hash": "sha512:085387365b7d3455e3d02fbd3dea753d9c28f0a8d19aa29ba32e7696e306e4ab186e23582fc7dca0572644931deb65cb15134284e3bf4d4ed725cd1ec299b443"
        }
    }
}
