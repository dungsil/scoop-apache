{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.74",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.74",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.74/bin/apache-tomcat-9.0.74-windows-x64.zip",
            "hash": "sha512:b5f5d09a5c4f98e7889cb6d2cb24a99aea21d503faa3d3769d29437145d3064c8c5ad2ed8b1c503ba50c3be1696d0db6be57f7b89ef4bb7884e3318c6462a996"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.74/bin/apache-tomcat-9.0.74-windows-x86.zip",
            "hash": "sha512:55e031f96311f4ccccf16198237d2149d840fef16116adf269eefa3070cc93bbeab225a5bf78de02bf358b89f93881c4ad57c943121c322ee395abd6ed89c2b6"
        }
    }
}
