{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.54",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.54",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.54/bin/apache-tomcat-9.0.54-windows-x64.zip",
            "hash": "sha512:9b6f439885bc0117aa37ff2c7229fa831e19319fe507e645d2c4b95eac32778c735a67b4db3282f65df339c4ada387d2919ce82430f9457512a9922752b6176b"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.54/bin/apache-tomcat-9.0.54-windows-x86.zip",
            "hash": "sha512:0d3c2c22cba17dd298a4f71eed145684d98c617ed8481b693275e69554521abf7554f49e61c8be7c4fcd20070aecc56c9d860e39c9c416a19579b6656bba613b"
        }
    }
}
