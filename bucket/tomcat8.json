{
    "homepage": "https://tomcat.apache.org",
    "version": "8.5.89",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-8/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-8/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-8/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-8.5.89",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-8/v8.5.89/bin/apache-tomcat-8.5.89-windows-x64.zip",
            "hash": "ef82098cd5b397dca2bc8b4f2094d1d945045bb3dc00fca4ac45c6e856299ae6"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-8/v8.5.89/bin/apache-tomcat-8.5.89-windows-x86.zip",
            "hash": "sha512:666bde674786ae2b6aac3f4445101dd5aba11073f2b4048b6732f3e88a7d0aef43b6ae853114af87c0cb24dfd9d95a54eea671a3641ea949dd3196a4c55ee024"
        }
    }
}
