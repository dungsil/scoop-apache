{
    "homepage": "https://tomcat.apache.org",
    "version": "8.5.82",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-8/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-8/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-8/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-8.5.82",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-8/v8.5.82/bin/apache-tomcat-8.5.82-windows-x64.zip",
            "hash": "sha512:830ce53d310268d7aa823936d16e48c9078e20975562c30222f5a774cfff3bbe6e80026bc1f5284a4b86f82ee211b86c3c6940f7a34bc85b743ad814c42ad8fa"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-8/v8.5.82/bin/apache-tomcat-8.5.82-windows-x86.zip",
            "hash": "sha512:1ace73b14dcd72660fab868a45e6ffa2537189c0d840afc31cf2885b904aba1356a30788d9d5bc38a87683ae5b379b63abe00baa2137a538aba2f7668f37eddf"
        }
    }
}
